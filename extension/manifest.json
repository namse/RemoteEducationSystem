{
    "manifest_version": 2,

    "name": "CurrentTabCaptureExtension",
    "description": "RemoteEducationSystemCurrentTabCaptureExtension",
    "version": "1.1",

    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "activeTab",
        "tabs",
        "<all_urls>"
    ],
    "content_scripts": [{
        "matches": ["https://61.38.158.172:1234/*"],
        "js": ["content.js"]
    }],
    "icons": {
        "128": "icon.png"
    },
    "web_accessible_resources": [
        "icon.png"
    ]
}